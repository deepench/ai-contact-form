{
  "name": "wpeverest/everest-forms-openai",
  "description": "Wordpress Plugin for Everest Forms AI.",
  "homepage": "https://wpeverest.com",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
  "php": ">=5.6|>=7.0",
  "composer/installers": "1.11.0",
  "madorin/matex": "dev-master"
  },
  "require-dev": {
  "wp-cli/i18n-command": "2.2.8",
  "wp-coding-standards/wpcs": "2.3.0",
  "dealerdirect/phpcodesniffer-composer-installer": "0.7.1",
  "phpcompatibility/phpcompatibility-wp": "2.1.1",
  "sirbrillig/phpcs-variable-analysis": "2.11.1"
  },
  "config": {
  "platform": {
  "php": "7.2"
  },
  "allow-plugins": {
  "composer/installers": true,
  "dealerdirect/phpcodesniffer-composer-installer": true
  }
  },
  "autoload": {
  "psr-4": {
  "EverestForms\\AI\\": "src/"
  }
  },
  "scripts": {
  "phpcs": [
  "phpcs"
  ],
  "phpcbf": [
  "phpcbf"
  ],
  "makepot-audit": [
  "wp --allow-root i18n make-pot . --slug=everest-forms-openai"
  ],
  "makepot": [
  "@makepot-audit --skip-audit"
  ]
  },
  "extra": {
  "scripts-description": {
  "phpcs": "Analyze code against the WordPress coding standard with PHP_CodeSniffer",
  "phpcbf": "Fix coding standards warnings/errors automattically with PHP Code Beautifier",
  "makepot-audit": "Generate langauges/everest-forms-openai.pot file and run audit",
  "makepot": "Generate langauges/everest-forms-openai.pot file"
  }
  }
  }
